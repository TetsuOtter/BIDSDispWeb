<Page
    x:Class="TR.BIDSDispWeb.MainPage"
  SizeChanged="OnSizeChange"
  Loaded="OnLoad"
  Unloaded="OnUnLoad"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
  Background="Black"
  UseLayoutRounding="True"
    mc:Ignorable="d">
  <Grid>
    <Frame Name="FrontFrame"
           UseLayoutRounding="True"
           Margin="0,0,0,0"
           Height="800"
           Width="1280">
      <Frame.UriMapper>
        <UriMapper>
          <UriMapping Uri="/"
                      MappedUri="TopPage.xaml"/>
          <UriMapping Uri="/Top"
                      MappedUri="TopPage.xaml"/>
        </UriMapper>
      </Frame.UriMapper>
    </Frame>
    <Rectangle   Name="LightAdjusterRec"
                 Opacity="0"
                 IsHitTestVisible="False"
                 Margin="0,0,0,0"
                 Fill="Black"/>

  </Grid>
</Page>
